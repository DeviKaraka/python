t = int(input())
for _ in range(t):
    n, l, r = map(int, input().split())
    a = list(map(int, input().split()))

    happiness = 0
    max_happiness = 0
    min_happiness = 0

    for x in a:
        if l <= x <= r:
            happiness += 1
        else:
            happiness -= 1

        max_happiness = max(max_happiness, happiness)
        min_happiness = min(min_happiness, happiness)
    print(max_happiness, min_happiness)
