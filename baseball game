class Solution(object):
    def calPoints(self, operations):
        count=0
        rec=[]
        for i in operations:
            if i=="C":
                rec.pop()
            elif i=="D":
                rec.append(2*rec[-1])
            elif i=="+":
                rec.append(rec[-1]+rec[-2])
            else:
                rec.append(int(i))
        return sum(rec)



       
